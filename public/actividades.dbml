Table actividaderivada {
    id int [pk, increment] // Primary key
    diagnostico_id int [ref: > diagnosticos.id]
    sesion varchar(50) 
    creacion_act date 
    eje varchar(100) 
    tema varchar(150) 
    actividades text 
    materiales_usar text 
    responsable_id int [ref: > tutores.id] 
    created_at timestamp
    updated_at timestamp
}